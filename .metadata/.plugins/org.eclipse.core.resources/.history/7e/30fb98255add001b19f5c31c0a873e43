import java.util.*;
public class LinkList {

	public static void main(String[] args) {
		LinkedList l=new LinkedList();
		for(int i=0;i<5;i++)
			l.add(i);
		System.out.println(l+"\n-----------\n");
		l.removeFirst();
		System.out.println(l+"\n-----------\n");
		l.addLast(10);
		System.out.println(l+"\n-----------\n");
		System.out.println("The number "+l.get(9));
		
	}

}
